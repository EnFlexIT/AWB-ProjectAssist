{
  "description": "This blueprint provides a basic structure for a main project.",
  "baseFolder": "featureBlueprint",
  "textReplacements": {
    "[SymBundleName]": "symBunName",
    "[BundleName]" : "bundleName",
    "[Project Name]" : "projectName"
  },
  "requiredArguments": [
    {
      "argumentName": "bundleName",
      "mandatory": true,
      "defaultValue": ""
    },
    {
      "argumentName": "symBunName",
      "mandatory": true,
      "defaultValue": ""
    },
    {
      "argumentName": "targetDir",
      "mandatory": true,
      "defaultValue": ""
    }		
  ]
}