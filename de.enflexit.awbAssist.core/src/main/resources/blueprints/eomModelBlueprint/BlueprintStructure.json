{
  "description": "This blueprint provides a basic structure for a eom model.",
  "baseFolder": "EomModelBlueprint",
  "textReplacements": {
    "[SymBundleName]": "symBunName",
    "[BundleName]" : "bundleName",
    "[technicalSystemName]" : "technicalSystemName"
  },
  "requiredArguments": [
    {
      "argumentName": "bundleName",
      "mandatory": true,
      "defaultValue": ""
    },
    {
      "argumentName": "symBunName",
      "mandatory": true,
      "defaultValue": ""
    },
    {
      "argumentName": "targetDir",
      "mandatory": true,
      "defaultValue": ""
    },
	{
	  "argumentName": "technicalSystemName",
	  "mandatory": true,
	  "defaultValue": ""
	}			
  ]
}